<?xml version="1.0"?>
<yandex>
    <users>
        <default>
            <password></password>
            <networks>
                <ip>::/0</ip> <!-- Cho phép tất cả IP trong docker network -->
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>

        <admin>
            <password>admin123</password>
            <networks>
                <ip>::/0</ip> <!-- Cho phép tất cả IP trong docker network -->
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </admin>
    </users>

    <profiles>
        <default>
            <max_threads>8</max_threads>
            <max_memory_usage>8G</max_memory_usage>
            <use_uncompressed_cache>1</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
        </default>
    </profiles>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</yandex>
